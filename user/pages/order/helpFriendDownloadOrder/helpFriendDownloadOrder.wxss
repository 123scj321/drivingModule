/* pages/order/helpFriendDownloadOrder/helpFriendDownloadOrder.wxss */
.relativeContainer{
  background: #ffffff;
  padding:25rpx 30rpx;
}
.nameAndPhoneAndisPaid{
  width:700rpx;
  font-size:32rpx;
  color:#dcdcdc;
}
.nameAndPhone{
  width:100%;
}
.nameAndPhone input{
  height:100rpx;
   border:2rpx solid #f9f9f9;
   padding-left: 25rpx;
   color: #888888;
}
.nameAndPhone text{
  display: block;
  width:100%;
  text-align: center;
  font-size:22rpx;
  height:70rpx;
  line-height: 70rpx;
  color:#888888;
}
.isPaid{
  width:700rpx;
  font-size:32rpx;
}
.isPaidContainer{
  display: flex;
  justify-content: space-between;
  width: 650rpx;
  height: 100rpx;
  line-height: 100rpx;
  border:2rpx solid #f9f9f9;
  padding-left: 25rpx;
  padding-right: 25rpx;
}
.isPaidContainer view{
color: #333333;
font-size: 32rpx;
}
.isPaid text{
  display: block;
  width:100%;
  text-align: center;
  font-size:22rpx;
  height:70rpx;
  line-height: 70rpx;
  color:#888888;
}
/* history record detail  container */
.historyRecord{
  margin-top:50rpx;
  width: 700rpx;
  color:#333333;
}
.historyRecordTitle{
  width:100%;
  font-size:32rpx;
  color:#333333;
  height: 50rpx;
  line-height: 50rpx;
  display: flex;
  justify-content: space-between;
  border-bottom:2rpx solid #dcdcdc;
}
.delete{
  padding-bottom: 10rpx;
  font-size:32rpx;
  color:#333333;
}
.delete icon{
  margin-bottom: 10rpx;
}
.historyRecordList{
  margin:0 auto;
  width:650rpx;
  font-size:28rpx;
  color: #333333;
}
.historyRecordItem{
  height:88rpx;
  line-height: 88rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #dcdcdc;
}
.historyRecordItem:last-child{
  border-bottom: none;
}
/*submitButton  */
.submitButton{
  margin-top:200rpx;
}
.submit{
  background: #0299de;
  color:#ffffff;
  font-size:36rpx;
  font-family: "微软雅黑";
}


