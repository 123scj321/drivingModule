/* pages/coupon/coupon.wxss */
page{
background:#f1f2f4;
  font-family: "微软雅黑";
  height:100%;
}

.couponTitle{
  font-size:28rpx;
  color:#555;
  padding:20rpx 20rpx;
}
/*couponList  */
.couponList{
  width:710rpx;
  margin:0 auto;
}
.couponItem{
  width:100%;
  height:157rpx;
  background: #fff;
  border-radius: 15rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom:35rpx;
}
/*reduceAndType  */
.reduceAndType{
  width:213rpx;
  border-right:4rpx dotted #fff;
  background: #f1953b;
  color:#fff;
  border-radius: 15rpx 0rpx 0rpx 15rpx;
  text-align: center;
}
.reduce{
  font-size:30rpx;
  margin-top:10rpx;
}
.reduce text:first-child{
  font-size:66rpx;
}
.reduce text:last-child{
  font-size:40rpx;
}
.type{
  font-size:26rpx;
}
/*couponDetail  */
.couponDetail{
  width:500rpx;
  font-size:24rpx;
  position: relative;
}
.couponDetail view{
  margin:15rpx;
}
.couponDetail image{
  width:175rpx;
  height:69rpx;
  position: absolute;
  bottom:0rpx;
  right:10rpx;
}